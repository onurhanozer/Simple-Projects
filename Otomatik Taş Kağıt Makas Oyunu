

import random

oyun_sayisi = int(input("Kaç Kez Oynansın :"))

i = 0

tas = 0
kagit = 0
makas = 0


while i <= oyun_sayisi:
    oyun = random.randint(1, 3)
    if (oyun == 1):
        tas += 1
    elif (oyun == 2):
       kagit += 1
    else:
       makas += 1
    i += 1

print("""

Taş {}
Kağıt {}
Makas {}

""".format(tas,kagit,makas))

